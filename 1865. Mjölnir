#include <stdio.h>
#include <string.h>

int main(){
    int casos;
    char nome [50];
    int forca;

    scanf("%d", &casos);

    while(casos > 0){
        scanf("%s %d\n", nome, &forca); // dá para usar o scanf porque os nomes não tem espaços
        if(strcmp(nome,"Thor") == 0){
           printf("Y\n");
        }else{
           printf("N\n");
        }
    casos--;
    }
  
    return 0;
}
